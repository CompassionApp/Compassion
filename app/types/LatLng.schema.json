{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "definitions": {},
  "description": "Geographical coordinate",
  "properties": {
    "lat": {
      "maximum": 90,
      "minimum": -90,
      "type": "number"
